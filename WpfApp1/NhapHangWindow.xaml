<Window x:Class="WpfApp1.NhapHangWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="NhapHangWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="28" Margin="280,94,0,0" TextWrapping="Wrap" Text="Mã đơn hàng" VerticalAlignment="Top" Width="224" TextChanged="tbxMaDonHang"/>
        <TextBox HorizontalAlignment="Left" Height="30" Margin="280,150,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="224" TextChanged="tbxSoLuong" Text="Số lượng"/>
        <Button Content="Xác nhận" HorizontalAlignment="Left" Height="36" Margin="347,239,0,0" VerticalAlignment="Top" Width="81" Click="btnXacNhan"/>

    </Grid>
</Window>
